<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>F# helpers for NLog</Description>
    <Version>4.7.12</Version>
    <Authors>Marcin Deptuła</Authors>
    <Owners>Marcin Deptuła</Owners>
    <TargetFrameworks>netstandard2.0;netstandard2.1;netcoreapp3.1;net5.0;net6.0</TargetFrameworks>
    <PackageId>NLog.FSharp</PackageId>
    <PackageTags>NLog FSharp F#</PackageTags>
    <PackageProjectUrl>https://github.com/Ravadre/NLog.FSharp</PackageProjectUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/Ravadre/NLog.FSharp</RepositoryUrl>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <RequireLicenseAcceptance>false</RequireLicenseAcceptance>
    <Copyright>Copyright Marcin Deptuła 2014-2018</Copyright>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="Logger.fs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="NLog" Version="4.7.12" />
  </ItemGroup>

</Project>
